<?php

/**
 * @file
 * FVDMUG Migrate installation code.
 */

/**
 * Implements hook_enable().
 */
function fvdmug_migrate_enable() {
  Migration::registerMigration('FvdmugImg');
  Migration::registerMigration('FvdmugCsv');
}

/**
 * Implements hook_disable().
 */
function fvdmug_migrate_disable() {
  Migration::deregisterMigration('FvdmugCsv');
  Migration::deregisterMigration('FvdmugImg');
}
